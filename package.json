    "smoke": "node test/smoke",
    "test": "node test/index",
    "tests": "node ./setup/nedb-setup && node test/index",
    "testd": "set DEBUG=express:* && node test/index"
  },
  "keywords": [
    "low code",
    "CRUD",
    "model-driven",
    "MDA",
    "REST",
    "platform",
    "metadata",
