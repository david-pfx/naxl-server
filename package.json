{
  "name": "naxl-server",
  "version": "0.1.1",
  "description": "Server backend for Naxl Personal Database Manager",
  "homepage": "https://github.com/david-pfx/naxl-server",
  "author": "David M. Bennett (naxl.net)",
  "repository": {
    "type": "git",
    "url": "git://github.com/david-pfx/naxl-server"
  },
  "bugs": "https://github.com/david-pfx/naxl-server/issues",
  "license": "MIT",
  "scripts": {
    "setup": "rm -f ./nedb-data/*.* && node ./setup/nedb-setup",
    "setup-sql": "node ./setup/database",
    "setup-nedb": "node ./setup/nedb-setup",
    "start": "supervisor -n exit ./bin/www",
    "startd": "set DEBUG=express:* && supervisor -n exit ./bin/www",
    "smokex": "node test/smoke",
    "smoke": "rm -f ../naxl-ui/public/pix/test/*.* && node test/smoke",
    "smokes": "rm -f ../naxl-ui/public/pix/test/*.* && node ./setup/nedb-setup && node test/smoke",
    "test": "rm -f ../naxl-ui/public/pix/test/*.* && node test/index",
    "tests": "rm -f ../naxl-ui/public/pix/test/*.* && node ./setup/nedb-setup && node test/index",
    "testd": "set DEBUG=express:* && node test/index"
  },
  "keywords": [
    "low code",
    "CRUD",
    "model-driven",
    "MDA",
    "REST",
    "platform",
    "metadata",
    "database",
    "nodejs",
    "ORM",
    "SQL",
    "generator",
    "express",
    "postgres",
    "javascript",
    "DRY"
  ],
  "dependencies": {
    "body-parser": "~1.18.3",
    "chalk": "~2.4.2",
    "csv-express": "~1.2.2",
    "csv-parse": "^4.3.3",
    "debug": "~4.1.1",
    "express": "~4.16.4",
    "formidable": "~1.2.1",
    "helmet": "~3.15.0",
    "nedb": "^1.8.0",
    "pg": "^7.8.0",
    "pg-connection-string": "~2.0.0",
    "shortid": "~2.2.14",
    "supertest": "^3.3.0",
    "supervisor": "~0.12.0",
    "tape": "^4.9.1"
  },
  "devDependencies": {}
}
