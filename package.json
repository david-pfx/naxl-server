  "dependencies": {
    "body-parser": "~1.18.3",
    "chalk": "~2.4.1",
    "csv-express": "^1.2.2",
    "debug": "~3.1.0",
    "express": "~4.16.3",
    "express-csv": "~0.6.0",
    "formidable": "~1.2.1",
    "helmet": "~3.13.0",
    "nedb": "^1.8.0",
    "pg": "^7.4.3",
    "pg-connection-string": "~2.0.0",
    "shortid": "~2.2.13",
